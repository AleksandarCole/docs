{
  "first-line-heading": false,

  "ul-style": {
    "style": "dash"
  },

  "line_length": {
    "code_blocks": false
  },

  "no-inline-html": {
    "allowed_elements": [
      "a",
      "table",
      "td",
      "tr",
      "th",
      "tr",
      "tbody",
      "thead",
      "sup"
    ]
  },

  "no-trailing-punctuation": {
    "punctuation": ".,;:!"
  }
}
